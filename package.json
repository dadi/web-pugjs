  },
  "homepage": "https://github.com/dadi/web-pugjs#readme",
  "dependencies": {
    "debug": "^2.6.6",
    "pug": "^2.0.0-rc.1",
    "require-dir": "^0.3.2",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "colors": "^1.1.2",
    "coveralls": "^3.0.3",
    "env-test": "^1.0.0",
    "html-looks-like": "^1.0.2",
    "mocha": "~3.2.0",
    "nyc": "^14.0.0",
    "object-path": "^0.11.4",
    "should": "~4.0.4",
    "sinon": "~1.17.6",
    "snazzy": "^6.0.0",
    "standard": "*"
